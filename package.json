{
  "name": "@urlaubsverwaltung/eslint-plugin",
  "version": "1.2.0",
  "description": "custom eslint rules for the open source urlaubsverwaltung application",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "urlaubsverwaltung"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Benjamin Seber <seber@synyx.de> (https://www.synyx.de)",
  "license": "Apache-2.0",
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "ava": "5.3.1",
    "eslint": "8.44.0",
    "eslint-ava-rule-tester": "4.0.0",
    "lint-staged": "13.2.3",
    "prettier": "3.0.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write '**/*.js'"
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  }
}
